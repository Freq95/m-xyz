# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Vecinu

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database (Supabase Postgres)
# IMPORTANT: Use Session Pooler (port 6543) with ?pgbouncer=true
# This prevents "prepared statement already exists" errors
DATABASE_URL=postgresql://postgres.[PROJECT-REF]:[PASSWORD]@aws-0-eu-central-1.pooler.supabase.com:6543/postgres?pgbouncer=true

# Direct connection (port 5432) - Required for Prisma migrations
# Uses the same pooler hostname but different port
DIRECT_URL=postgresql://postgres.[PROJECT-REF]:[PASSWORD]@aws-0-eu-central-1.pooler.supabase.com:5432/postgres

# Email (Resend) - Optional for MVP
RESEND_API_KEY=re_your-api-key
EMAIL_FROM=noreply@vecinu.ro

# Rate Limiting (Upstash Redis) - Optional for MVP
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-token

# Background Jobs (Inngest) - Optional for MVP
INNGEST_EVENT_KEY=your-event-key
INNGEST_SIGNING_KEY=your-signing-key
